{
  "name": "users-gql",
  "version": "2.2.0",
  "engines": {
    "node": "16.x"
  },
  "license": "MIT",
  "scripts": {
    "start": "node dist/main",
    "dev": "ts-node-dev --respawn --transpile-only --clear -r dotenv-flow/config --inspect=0.0.0.0:9229 src/main",
    "clean": "rimraf dist",
    "build": "npm run clean && npx tsc -p tsconfig-build.json",
    "postbuild": "npx prisma migrate deploy --preview-feature",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --passWithNoTests --noStackTrace --runInBand --no-cache",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "dependencies": {
    "@prisma/client": "^2.29.1",
    "apollo-server-express": "^3.2.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@jest-mock/express": "^1.4.4",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv-flow": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.0.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.7.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "faker": "^5.5.3",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-environment-node": "^27.0.6",
    "jest-mock-extended": "^2.0.2-beta2",
    "lint-staged": "^11.1.2",
    "pg": "^8.7.1",
    "prisma": "^2.29.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  }
